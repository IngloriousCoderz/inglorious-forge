exports[`@inglorious/vite-plugin-vue > complex component > handles complete todo app 1`] = `
"import { html, when, repeat } from "@inglorious/web";

export const testComponent = {
  create(entity) {
    entity.todos = []
    entity.newTodo = ''
  },

  addTodo(entity, e) {
    e.preventDefault()
      if (!entity.newTodo.trim()) return
      entity.todos.push({
        id: Date.now(),
        text: entity.newTodo,
        done: false
      })
      entity.newTodo = ''
  },

  updateNewTodo(entity, e) {
    entity.newTodo = e.target.value
  },

  toggleTodo(entity, id) {
    const todo = entity.todos.find(t => t.id === id)
      if (todo) todo.done = !todo.done
  },

  removeTodo(entity, id) {
    entity.todos = entity.todos.filter(t => t.id !== id)
  },

  render(entity, api) {
    return html\`<div class="todos">
    html\`<form @submit="\${() => api.notify(\`#\${entity.id}:addTodo\`)}">
      html\`<input .value="\${entity.newTodo}" @input="\${() => api.notify(\`#\${entity.id}:updateNewTodo\`)}"></input>\`
      html\`<button type="submit">Add</button>\`
    </form>\`
    \${when(entity.todos.length > 0, () => html\`<ul>
      \${repeat(entity.todos, todo => todo.id, (todo) => html\`<li>
        html\`<input type="checkbox" .checked="\${todo.done}" @change="\${() => api.notify(\`#\${entity.id}:toggleTodo\`, todo.id)}"></input>\`
        html\`<span>\${todo.text}</span>\`
        html\`<button @click="\${() => api.notify(\`#\${entity.id}:removeTodo\`, todo.id)}">Ã—</button>\`
      </li>\`)}
    </ul>\`, () => html\`<p>No todos yet!</p>\`)}
    html\`<p>No todos yet!</p>\`
  </div>\`;
  }
};
"
`;

